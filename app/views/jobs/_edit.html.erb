<% provide(:title, "Edit job") %>
<h1>Update job</h1>

<div class="row">
        <div class="span6 offset3">
        <%= form_for(@job) do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%= f.label :logdate %>
        <%= f.text_field :logdate %>

        <%= f.label :poolname %>
        <%= f.text_field :poolname %>

        <%= f.label :serverlist %>
        <%= f.text_field :serverlist %>

        <%= f.label :failtime %>
        <%= f.text_field :failtime %>

        <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
        <% end %>
        </div>
        </div>